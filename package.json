  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "npm run build:dist && node build/build.js && node build/move-headcss.js",
    "build:formation": "rollup -c build/rollup.config.js",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build:dist": "webpack --bail --progress --hide-modules --config  build/webpack.build.min.js && webpack --bail --progress --hide-modules --config  build/webpack.build.js"
